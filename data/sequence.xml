<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
        </record>
    </data>

    <data noupdate="1">
        <record id="sequence_hospital_doctor" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">hospital.doctor.sequence</field>
            <field name="prefix">D</field>
            <field name="padding">5</field>
        </record>
    </data>

    <data noupdate="1">
        <record id="sequence_hospital_hospital" model="ir.sequence">
            <field name="name">Hospital Sequence</field>
            <field name="code">hospital.hospital.sequence</field>
            <field name="prefix">H</field>
            <field name="padding">4</field>
        </record>
    </data>

    <data noupdate="1">
        <record id="sequence_hospital_patient_history" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">hospital.patient.history.sequence</field>
            <field name="prefix">APT</field>
            <field name="padding">7</field>
        </record>
    </data>
</odoo>